countrytogdprank = containers.Map();
[id, country, GDP] = csvimport('GDPByCountryformatlab.csv', 'columns',  {'Ranking', 'Economy','Size'});
for c = 1:length(country)
    current = country(c);
    countrytogdprank(char(current)) = id(c);
end
